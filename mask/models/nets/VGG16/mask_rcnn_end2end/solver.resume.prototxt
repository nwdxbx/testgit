train_net: "models/tuzhen/VGG16/mask_rcnn_end2end/train.prototxt"
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 100000
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "vgg16_mask_keypoints_0.0001_resume"
iter_size: 2
